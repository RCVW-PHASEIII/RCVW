{
	"name":"@TMX_PLUGIN_NAME@",
	"description":"Sends HRI Status as SPAT Message",
	"version":"@PROJECT_VERSION@",
	"exe": "bin/@PROJECT_NAME@",
	"channels":[
		{
			"context": "kafka://localhost:29092"
		},
		{
			"comment": "TMX CBS cloud connection. Sending generated messages and status",
			"id": "tmx-cbs",
			"context": "amqps://CBSReceiver:hdJ+udbmcmgoM9BZvGm/0P0yHpXKt/NZt+ASbAxAnwg=@rcvwcbssb.servicebus.windows.net",
			"config": {
				"auto-subscribe": false,
				"write-only": true,
				"topics": "^(J2735|tmx)/",
